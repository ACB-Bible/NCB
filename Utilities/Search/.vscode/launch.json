{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome search.html Launch",
            "url": "http://localhost:5500/HTML/search.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node search-node.js Launch",
            "program": "${workspaceFolder}/JS/search-node.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
        }
    ]
}