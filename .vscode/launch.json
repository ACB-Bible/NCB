{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome index.html",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:5500/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Chrome textpage.html",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:5500/ASSETS/textpage.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Chrome compress.html",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:5500/Utilities/Compress/HTML/compress.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node compress-node.js Launch",
            "program": "${workspaceFolder}/Utilities/Compress/JS/compress-node.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
        }
    ]
}