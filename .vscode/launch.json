    "version": "0.2.0",
    "configurations": [
        {
            "name": "Edge index.html",
            "request": "launch",
            "type": "msedge",
            "url": "http://127.0.0.1:5500/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Chrome index.html",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:5501/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Chrome textpage.html",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:5501/ASSETS/textpage.html",
            "webRoot": "${workspaceFolder}"
        },
    ]
}